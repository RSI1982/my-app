node{
  stage('git checkout'){
    git credentialsId: 'GITCredential', url: 'https://github.com/RSI1982/my-app.git'
  }
  stage('build and test'){
    def mvnhome = tool name: 'mavenhome', type: 'maven'
    sh "${mvnhome}/bin/mvn package"
  }
    stage ('Creating build tag') {
        createTag nexusInstanceId: 'nexus125', tagAttributesJson: '{"createdBy" : "rakeshsingh"}', tagName: 'build-125'
  }
}
